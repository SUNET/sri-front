    "scripts": {
        "start": "NODE_ENV=dev BROWSER=none react-scripts start",
        "build": "react-scripts build",
        "test": "TZ=UTC react-scripts test --watchAll=false",
        "test:watch": "TZ=UTC react-scripts test --watchAll",
        "eject": "react-scripts eject",
        "check-code": "prettier --check ./src/**/*.{js,jsx,json}",
        "check-styles": "prettier --check --config .stylelintrc ./src/**/*.{css,scss}",
        "fix-code": "prettier --write ./src/**/*.{js,jsx,json}",
        "fix-styles": "prettier --write --config .stylelintrc ./src/**/*.{css,scss}",
        "relay": "relay-compiler --src ./src --schema ./schema.graphql --extensions js jsx --watch",
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "generate-schema": "graphql get-schema",
        "create-entity-list": "node ./cli-entity/cli.js generate-list",
        "create-entity-details": "node ./cli-entity/cli.js generate-details"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "babelMacros": {
        "styledComponents": {
            "pure": true
        }
    }
